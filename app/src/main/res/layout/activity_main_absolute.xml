<?xml version="1.0" encoding="utf-8"?>
<android.widget.AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivityAbsolute">


    <ImageView
        android:id="@+id/imageViewAbsolute"
        android:layout_width="196dp"
        android:layout_height="165dp"
        android:layout_x="88dp"
        android:layout_y="64dp"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_editor_absoluteX="134dp"
        tools:layout_editor_absoluteY="38dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="109dp"
        android:layout_y="23dp"
        android:text="Welcome To This Page"
        tools:layout_editor_absoluteX="183dp"
        tools:layout_editor_absoluteY="24dp" />

    <TextView
        android:id="@+id/textViewAbsolute"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="98dp"
        android:layout_y="261dp"
        android:text="Guess the layout of this Page"
        tools:layout_editor_absoluteX="178dp"
        tools:layout_editor_absoluteY="266dp" />

    <EditText
        android:id="@+id/editTextAbsolute"
        android:layout_width="328dp"
        android:layout_height="wrap_content"
        android:layout_x="30dp"
        android:layout_y="301dp"
        android:ems="10"
        android:hint="example: absolute"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="300dp" />

    <Button
        android:id="@+id/buttonAbsolute"
        android:layout_width="325dp"
        android:layout_height="wrap_content"
        android:layout_x="33dp"
        android:layout_y="366dp"
        android:onClick="fnCheck"
        android:text="Check"
        tools:layout_editor_absoluteX="192dp"
        tools:layout_editor_absoluteY="379dp" />
</android.widget.AbsoluteLayout>